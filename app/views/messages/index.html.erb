<div class="panel panel-default">
  <div class="panel-heading panel1">
    メッセージ一覧
  </div>
<!-- - - - - - - - - - - - - - - - メッセージ履歴表示部- - - - - - - - - - - - - -->    
  <div class="panel-body panel1">
    <% @messages.each do |message| %>
      <span class="label label-info"><%= message.name %></span>
      <%= message.body %>
      <br>
    <% end %>
  </div>
</div>

<!-- 以下は_form.html.erbに移動
  ↓　_form.html.erbの呼び出し -->

<%= render 'form' %>